    ERC20 public BUSD = ERC20(0x2E50a44F2C744E2BcDe025028622d6349115D7Bf);
    ERC20 public USDT = ERC20(0x24DA85920bbF1be872632aF232ac34a2C5580Ef6);
    ERC20 public AMBAR = ERC20(0x0bcaEaB8160482801D5bC3f57ee5ED5caB2458ae);

