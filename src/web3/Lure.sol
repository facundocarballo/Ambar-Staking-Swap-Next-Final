    function withdraw_busd(uint256 amount) public {
        require(busdOf[msg.sender] >= amount, MSG_WITHDRAW_FAIL);
        require(get_amount_of_our_busd() >= amount, MSG_NO_FUNDS);
        require(BUSD.transfer(msg.sender, amount), MSG_BUSD_FAIL);

        busdOf[msg.sender] -= amount;
    }

    function withdraw_usdt(uint256 amount) public {
        require(usdtOf[msg.sender] >= amount, MSG_WITHDRAW_FAIL);
        require(get_amount_of_our_usdt() >= amount, MSG_NO_FUNDS);
