    function deposit_busd(uint256 amount) public {
        require(
            BUSD.transferFrom(msg.sender, ownerWallet, amount),
            MSG_BUSD_FAIL
        );
        busdOf[msg.sender] += amount;
    }

    function deposit_usdt(uint256 amount) public {
        require(
            USDT.transferFrom(msg.sender, ownerWallet, amount),
            MSG_USDT_FAIL
        );
        usdtOf[msg.sender] += amount;
    }

    function deposit_ambar(uint256 amount) public {
        require(
            AMBAR.transferFrom(msg.sender, ownerWallet, amount),
            MSG_AMBAR_FAIL
        );
        ambarOf[msg.sender] += amount;
    }

    function deposit_bnb() public payable {
        bnbOf[msg.sender] += msg.value;
    }

